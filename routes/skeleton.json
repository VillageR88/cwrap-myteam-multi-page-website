{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "favicon.ico",
        "type": "image/x-icon"
      }
    ]
  },
  "element": "body",
  "style": "box-sizing: border-box; display: flex; flex-direction: column; font-family: 'Livvic', serif; margin: 0; min-height: 100dvh; padding: 0;",
  "children": [
    {
      "element": "nav",
      "style": "background-color: #014E56; display: flex; min-height: 241px; width: 100%;",
      "children": [
        {
          "element": "div",
          "style": "align-items: center; display: flex; justify-content: space-between; margin-block: 73px auto; margin-inline: auto; max-width: 1110px; width: 100%;",
          "children": [
            {
              "element": "div",
              "style": "display: flex; gap: 80px;",
              "children": [
                {
                  "element": "img",
                  "attributes": {
                    "alt": "My team logo",
                    "src": "cwrapProperty[prefix=./]static/images/logo.svg"
                  }
                },
                {
                  "element": "ul",
                  "style": "align-items: center; box-sizing: border-box; display: flex; gap: 40px; list-style: none; margin: 0; padding: 0;",
                  "blueprint": {
                    "count": "2",
                    "element": "li",
                    "children": [
                      {
                        "element": "a",
                        "extend": [
                          {
                            "extension": ":hover",
                            "style": "color: #F67E7E"
                          }
                        ],
                        "attributes": {
                          "class": "text-description-size1",
                          "href": "cwrapProperty[prefix=./]cwrapArray[,about]"
                        },
                        "style": "color: white; text-decoration: none; transition: 300ms;",
                        "text": "cwrapArray[home,about]"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "element": "a",
              "attributes": {
                "class": "text-description-size1",
                "href": "cwrapProperty[prefix=./]contact-us"
              },
              "extend": [
                {
                  "extension": ":hover",
                  "style": "color: #002529; background-color: #FFFFFF;"
                }
              ],
              "style": "align-items: center; border-radius: 24px; border: 2px solid white; color: white; display: flex; justify-content: center; max-width: 153px; min-height: 48px; text-decoration: none; transition: 300ms; width: 100%;",
              "text": "contact us"
            }
          ]
        }
      ]
    },
    {
      "element": "main",
      "style": "display: flex; flex-direction: column; overflow-x: clip; width: 100%;",
      "children": [
        {
          "element": "div",
          "style": "background-color: #014E56;",
          "children": [
            {
              "element": "div",
              "style": "align-items: center; display: flex; margin-inline: auto; max-width: 1110px; min-height: 450px; position: relative; width: 100%;",
              "children": [
                {
                  "element": "img",
                  "style": "bottom: 0; position: absolute; right: 0;",
                  "attributes": {
                    "alt": "background pattern",
                    "src": "static/images/bg-pattern-home-2.svg"
                  }
                },
                {
                  "element": "div",
                  "style": "display: flex; gap: 24px; justify-content: space-between; margin-bottom: auto; position: relative; width: 100%;",
                  "children": [
                    {
                      "element": "img",
                      "style": "left: calc(-200px - 65px); position: absolute;",
                      "attributes": {
                        "alt": "background pattern",
                        "src": "static/images/bg-pattern-home-1.svg"
                      }
                    },
                    {
                      "element": "h1",
                      "style": "color: white; font-size: 100px; font-weight: bold; line-height: 100px; margin: 0; max-width: 615px; width: 100%;",
                      "text": "cwrapGlobal[home.hero.h1Title] cwrapSpan",
                      "children": [
                        {
                          "element": "span",
                          "text": "cwrapGlobal[home.hero.h1TitleDistinction]",
                          "style": "color: #F67E7E;"
                        }
                      ]
                    },
                    {
                      "element": "div",
                      "style": "display: flex; flex-direction: column; justify-content: space-between; max-width: 445px; width: 100%;",
                      "children": [
                        {
                          "element": "div",
                          "style": "background-color: #79C8C7; height: 4px; width: 50px;"
                        },
                        {
                          "element": "p",
                          "style": "color: white;",
                          "attributes": {
                            "class": "text-description-size1"
                          },
                          "text": "cwrapGlobal[home.hero.description]"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "element": "div",
          "style": "background-color: #012F34;",
          "children": [
            {
              "element": "div",
              "style": "display: flex; justify-content: space-between; margin-block: 140px; margin-inline: auto; max-width: 1110px; min-height: 404px; position: relative; width: 100%;",
              "children": [
                {
                  "element": "img",
                  "style": "bottom: -140px; position: absolute; right: calc(-65px - 200px);",
                  "attributes": {
                    "src": "static/images/bg-pattern-home-3.svg"
                  }
                },
                {
                  "element": "div",
                  "style": "display: flex; flex-direction: column; gap: 54px; max-width: 445px;",
                  "children": [
                    {
                      "element": "div",
                      "style": "background-color: #F67E7E; height: 4px; width: 50px;"
                    },
                    {
                      "element": "h2",
                      "style": "color: white;",
                      "text": "cwrapGlobal[home.buildNManage.h2Title]"
                    }
                  ]
                },
                {
                  "element": "ul",
                  "style": "box-sizing: border-box; display: flex; flex-direction: column; gap: 32px; list-style: none; margin: 58px 0 0 0; max-width: 540px; padding: 0px;",
                  "blueprint": {
                    "count": "3",
                    "element": "li",
                    "style": "align-items: start; display: flex; gap: 23px;",
                    "children": [
                      {
                        "element": "img",
                        "attributes": {
                          "src": "cwrapGlobal[home.buildNManage.features.cwrapIndex.image]"
                        }
                      },
                      {
                        "element": "section",
                        "style": "display: flex; flex-direction: column; gap: 16px;",
                        "children": [
                          {
                            "element": "h3",
                            "style": "color: #F67E7E;",
                            "text": "cwrapGlobal[home.buildNManage.features.cwrapIndex.title]"
                          },
                          {
                            "element": "p",
                            "style": "color: white; opacity: 0.8;",
                            "text": "cwrapGlobal[home.buildNManage.features.cwrapIndex.description]"
                          }
                        ]
                      }
                    ]
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}