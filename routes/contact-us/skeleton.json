{
  "head": {
    "link": [
      {
        "rel": "shortcut icon",
        "href": "../favicon.ico",
        "type": "image/x-icon"
      },
      {
        "rel": "prefetch",
        "href": "../"
      },
      {
        "rel": "prefetch",
        "href": "../about"
      }
    ]
  },
  "classroom": [
    {
      "name": "placeholder",
      "type": "pseudo::",
      "style": "color: #FFFFFF; opacity: 0.6;"
    },
    {
      "name": ":has(input:user-invalid) ::placeholder",
      "type": "custom",
      "style": "color: #F67E7E; opacity: 0.6;"
    },
    {
      "name": "input:-webkit-autofill",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white;"
    },
    {
      "name": "input:-webkit-autofill:hover",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white;"
    },
    {
      "name": "input:-webkit-autofill:focus",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white;"
    },
    {
      "name": "input:-webkit-autofill:active",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white;"
    },
    {
      "name": "input::-webkit-autofill",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white; transition: background-color 500s ease-in-out 0s;"
    },
    {
      "name": "input::-webkit-autofill:hover",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white; transition: background-color 500s ease-in-out 0s;"
    },
    {
      "name": "input::-webkit-autofill:focus",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white; transition: background-color 500s ease-in-out 0s;"
    },
    {
      "name": "input::-webkit-autofill:active",
      "type": "element",
      "style": "-webkit-box-shadow: 0 0 0 300px #014E56 inset; -webkit-text-fill-color: white; transition: background-color 500s ease-in-out 0s;"
    },
    {
      "name": "input::before",
      "type": "element",
      "style": "background-color: #014E56; content: ''; display: block; height: 100%; opacity: 0.6; width: 100%;"
    },
    {
      "name": "input::after",
      "type": "element",
      "style": "background-color: #014E56; content: ''; display: block; height: 100%; opacity: 0.6; width: 100%;"
    }
  ],
  "element": "cwrap-template",
  "text": "cwrapTemplate[body(prefix=../,omitFooterFirst=cwrapOmit)]",
